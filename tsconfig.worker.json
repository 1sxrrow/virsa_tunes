{
    "compilerOptions": {
      "target": "ES2022",
      "module": "ES2022",
      "lib": ["ES2022", "webworker"],
      "moduleResolution": "node",
      "outDir": "./dist/out-tsc/worker",
      "strict": true,
      "skipLibCheck": true,
      "resolveJsonModule": true,
      "allowSyntheticDefaultImports": true
    },
    "include": ["src/app/shared/workers/**/*.ts"]
  }